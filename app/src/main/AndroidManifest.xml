<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.example.rahul.animationdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".animActivity.ImageClick" />
        <activity android:name=".animActivity.BubbleButton" />
        <activity android:name=".animActivity.EmptyActivity" />
        <activity android:name=".animActivity.SharedElement" />
        <activity android:name=".animActivity.ViewAnimation" />
        <activity android:name=".animActivity.TransitionActivity" />
        <activity android:name=".animActivity.RippleEffect" />
        <activity android:name=".animActivity.VisibilityActivity" />
        <activity android:name=".animActivity.VibrationActivity" />
        <activity android:name=".AnimationActivity"/>
        <activity android:name=".animActivity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
            </intent-filter>
        </activity>
        <activity android:name=".animActivity.AnimEnlargectivity"/>
    </application>

</manifest>