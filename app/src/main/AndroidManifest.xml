<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.example.rahul.animationdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ImageClick" />
        <activity android:name=".BubbleButton" />
        <activity android:name=".EmptyActivity" />
        <activity android:name=".SharedElement" />
        <activity android:name=".ViewAnimation" />
        <activity android:name=".TransitionActivity" />
        <activity android:name=".RippleEffect" />
        <activity android:name=".VisibilityActivity" />
        <activity android:name=".VibrationActivity" />
    </application>

</manifest>